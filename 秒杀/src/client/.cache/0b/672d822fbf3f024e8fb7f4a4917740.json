    transition: 0.25s;\r\n}\r\n\r\n.prev,\r\n.next {\r\n    position: absolute;\r\n    top: 0;\r\n    height: 63px;\r\n    width: 20px;\r\n    background-color: #333;\r\n    background-size: 100%;\r\n    background-repeat: no-repeat;\r\n    background-image: none;\r\n}\r\n\r\n.prev {\r\n    left: 0;\r\n}\r\n\r\n.next {\r\n    right: 0;\r\n}\r\n\r\n.prev:hover {\r\n    background-color: #666;\r\n    background-image: url(../banner/img/prev.png);\r\n    background-position: -5px 20px;\r\n}\r\n\r\n.next:hover {\r\n    background-color: #666;\r\n    background-image: url(../banner/img/next.png);\r\n    background-position: 5px 20px;\r\n}\r\n\r\n.list a {\r\n    float: left;\r\n    background: #333;\r\n    color: #666;\r\n    padding: 10px 12px;\r\n    text-align: center;\r\n}\r\n\r\n.list .time {\r\n    font-size: 18px;\r\n    font-weight: bold;\r\n}\r\n\r\n.content {\r\n    width: 1000px;\r\n    margin: 10px auto;\r\n}\r\n\r\n#mian {\r\n    width: 800px;\r\n}\r\n\r\n.meun li {\r\n    float: left;\r\n    width: 280px;\r\n    padding: 10px 15px;\r\n    margin: 20px 20px 20px 0;\r\n    background-color: #fff;\r\n}\r\n\r\n.meun img {\r\n    width: 100%;\r\n    height: 250px;\r\n}\r\n\r\n.meun a {\r\n    color: black;\r\n}\r\n\r\n.meun p {\r\n    overflow: hidden;\r\n    text-overflow: ellipsis;\r\n    display: -webkit-box;\r\n    -webkit-line-clamp: 2;\r\n    -webkit-box-orient: vertical;\r\n    line-height: 20px;\r\n}\r\n\r\n.meun em {\r\n    color: #ff4400;\r\n}\r\n\r\n.meun em span {\r\n    font-size: 28px;\r\n}\r\n\r\n.meun em+span {\r\n    font-size: 12px;\r\n    color: #666;\r\n    text-decoration: line-through;\r\n    margin-right: 80px;\r\n}\r\n\r\n.meun button {\r\n    padding: 10px 15px;\r\n    background-color: #ff4500;\r\n    border-radius: 6px;\r\n    color: #fff;\r\n}\r\n\r\n#page {\r\n    width: 700px;\r\n    height: 40px;\r\n    border: 1px solid red;\r\n}\r\n\r\n#side {\r\n    width: 198px;\r\n    height: 500px;\r\n    border: 1px solid red;\r\n}\r\n\r\n#side h3 {\r\n    height: 30px;\r\n    line-height: 30px;\r\n    text-align: center;\r\n    padding: 10px 0;\r\n}\r\n\r\n#side ul {\r\n    width: 100%;\r\n    border: 1px solid red;\r\n}\r\n\r\n#side li {\r\n    height: 30px;\r\n    line-height: 30px;\r\n    border-bottom: 1px solid #999;\r\n}"},"lineCount":null}},"error":null,"hash":"1384be995c507b50adf65cee83550f2f","cacheData":{"env":{}}}